<div class ="header">
<h2 class="title">Check list</h2>

<%= link_to 'タスク作成', '/new', class: "btn" %>
</div>



<% @task.each do |task| %>
  <div class="cp_ipcheck">
  <label class="tag">
  <input type="checkbox" class="option-input02 checkbox" checked />
  <p class = "task_name" ><%= task.name %></p>
  </label>
  <%= link_to '削除', "/tasks/#{task.id}", method: :delete, :class => 'btn main' %>
  </div>
<% end %>





